<?xml version="1.0" encoding="UTF-8"?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">biodata_record_biodata_addrow.biodata_record_biodata_addrow_data_source</item>
            <item name="deps" xsi:type="string">biodata_record_biodata_addrow.biodata_record_biodata_addrow_data_source</item>
        </item>
        <item name="label" xsi:type="string" translate="true">Biodata Information</item>
        <item name="config" xsi:type="array">
            <item name="dataScope" xsi:type="string">data</item>
            <item name="namespace" xsi:type="string">biodata_record_biodata_addrow</item>
        </item>
        <item name="template" xsi:type="string">templates/form/collapsible</item>
    </argument>
    <settings>
        <buttons>
            <button name="back" class="Surbhitest\Biodata\Block\Adminhtml\Biodata\Edit\BackButton"/>
            <button name="reset" class="Surbhitest\Biodata\Block\Adminhtml\Biodata\Edit\ResetButton"/>
            <button name="save" class="Surbhitest\Biodata\Block\Adminhtml\Biodata\Edit\SaveButton"/>
            <button name="delete" class="Surbhitest\Biodata\Block\Adminhtml\Biodata\Edit\DeleteButton"/>
        </buttons>
        <namespace>biodata_record_biodata_addrow</namespace>
        <dataScope>data</dataScope>
        <deps>
            <dep>biodata_record_biodata_addrow.biodata_record_biodata_addrow_data_source</dep>
        </deps>
    </settings>
    <dataSource name="biodata_record_biodata_addrow_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
          <argument name="class" xsi:type="string">Surbhitest\Biodata\Model\DataProvider</argument>
            <argument name="name" xsi:type="string">biodata_record_biodata_addrow_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">biodata_id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
        </argument>
        <argument name="data" xsi:type="array">
            <item name="js_config" xsi:type="array">
                <item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
            </item>
        </argument>
         <settings>
          <submitUrl path="*/*/save"/>
        </settings>
    </dataSource>
    <fieldset name="biodata_details">
       <settings>
            <collapsible>false</collapsible>
            <label/>
        </settings>
