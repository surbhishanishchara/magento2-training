<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceContainer name="content">
        <block class="Surbhitest\TestData\Block\Display" name="testdata_display" template="Surbhitest_TestData::frontmessage.phtml" >
            <arguments>
                <argument name ="show_msg" xsi:type='string'>Show Message From Argument</argument>
                <argument name ="show_grade" xsi:type='array'>
                    <item name ="first" xsi:type="string">A+</item>
                    <item name ="second" xsi:type="string">A</item>
                    <item name ="third" xsi:type="string">B+</item>
                    <item name ="four" xsi:type="string">B</item>
                </argument>
            </arguments>
        </block>
    </referenceContainer>
</page>